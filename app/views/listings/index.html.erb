<div class="container">
  <h1>Listings</h1>
  <div class="filters">
    <p>Searchbar</p>
    <p>Filter Dropdown</p>
  </div>
  <div class="row">
    <% @listings.each do |listing| %>
      <div class="col-3 mb-3">
        <%= render "listings/card", listing: listing %>
      </div>
    <% end %>
  </div>
</div>

